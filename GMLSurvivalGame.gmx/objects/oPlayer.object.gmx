            <string>///Inventory
//this code will be quite bulky whatever
scrControls();
if(INVENTORY){
    if(inventory_opened == true)inventory_opened = false else inventory_opened = true;
}

if(inventory_opened == true and LPMOUSE){
    var k = -1;
    for(var i = 0; i &lt; max_secondary_inventory;i++){
    if(i%8 == 0)k++
        if(point_in_rectangle(mouse_x,mouse_y,32+view_xview+(k*16),80+((i - (8*k))*16)+view_yview,32+16+view_xview+(k*16),96+((i - (8*k))*16)+view_yview)){
       
            if(has_in_hand[0] = -1){
                has_in_hand[0] = secondary_inventory_array[i,0];
                has_in_hand[1] = secondary_inventory_array[i,1];
                has_in_hand[2] = secondary_inventory_array[i,2];
                secondary_inventory_array[i,0] = -1;
                econdary_inventory_array[i,1] = 0;
                secondary_inventory_array[i,2] = 0;
            } else if(secondary_inventory_array[i,0] = -1){
                secondary_inventory_array[i,0] = has_in_hand[0];
                secondary_inventory_array[i,1] = has_in_hand[1];
                secondary_inventory_array[i,2] = has_in_hand[2];
                has_in_hand[0] = -1;
                has_in_hand[1] = 0;
                has_in_hand[2] = 0;
            }else {
            var temp0 = secondary_inventory_array[i,0];
            secondary_inventory_array[i,0] = has_in_hand[0];
            has_in_hand[0]=temp0
            temp0 = secondary_inventory_array[i,1];
            secondary_inventory_array[i,1] = has_in_hand[1];
            has_in_hand[1]=temp0
            temp0 = secondary_inventory_array[i,2];
            secondary_inventory_array[i,2] = has_in_hand[2];
            has_in_hand[2]=temp0
            }
       }
    }
    for(var i = 0; i &lt; max_inventory;i++){
        if(point_in_rectangle(mouse_x,mouse_y,8+view_xview,80+(i*16)+view_yview,8+16+view_xview,96+(i*16)+view_yview)){
       
            if(has_in_hand[0] = -1){
                has_in_hand[0] = inventory_array[i,0];
                has_in_hand[1] = inventory_array[i,1];
                has_in_hand[2] = inventory_array[i,2];
                inventory_array[i,0] = -1;
                inventory_array[i,1] = 0;
                inventory_array[i,2] = 0;
            } else if(inventory_array[i,0] = -1){
                inventory_array[i,0] = has_in_hand[0];
                inventory_array[i,1] = has_in_hand[1];
                inventory_array[i,2] = has_in_hand[2];
                has_in_hand[0] = -1;
                has_in_hand[1] = 0;
                has_in_hand[2] = 0;
            }else {
            var temp0 = inventory_array[i,0];
            inventory_array[i,0] = has_in_hand[0];
            has_in_hand[0]=temp0
            temp0 = inventory_array[i,1];
            inventory_array[i,1] = has_in_hand[1];
            has_in_hand[1]=temp0
            temp0 = inventory_array[i,2];
            inventory_array[i,2] = has_in_hand[2];
            has_in_hand[2]=temp0
            }
       }
    }
}


itemUnderPlayer = instance_place(x,y,oBaseItem);
if(itemUnderPlayer != noone){
    for(var i = 0; i &lt; max_inventory;i++){
        if(inventory_array[i,0] == itemUnderPlayer.image_index and inventory_array[i,1] &lt; inventory_array[i,2]){
            inventory_array[i,1]++
            with(itemUnderPlayer){instance_destroy();
            }
            itemUnderPlayer = noone;
            break;
        }
        if(inventory_array[i,0] = -1){
            inventory_array[i,0] = itemUnderPlayer.image_index;
            inventory_array[i,1]++;
            inventory_array[i,2] = itemUnderPlayer.max_stack;
            
            with(itemUnderPlayer){instance_destroy();
            }
            itemUnderPlayer = noone;
            break;
        }
    }

    if(itemUnderPlayer != noone){
        for(var i = 0; i &lt; max_secondary_inventory;i++){
            if((secondary_inventory_array[i,0] == itemUnderPlayer.image_index) and secondary_inventory_array[i,1] &lt; secondary_inventory_array[i,2]){
                secondary_inventory_array[i,1]++
                with(itemUnderPlayer){instance_destroy();
                }
                itemUnderPlayer = noone;
                break;
            }
            if(secondary_inventory_array[i,0] = -1){
                secondary_inventory_array[i,0] = itemUnderPlayer.image_index;
                secondary_inventory_array[i,1]++;
                secondary_inventory_array[i,2] = itemUnderPlayer.max_stack;
                with(itemUnderPlayer){instance_destroy();
                }
                itemUnderPlayer = noone;
                break;
            }
        }
    }
}
//scrolling
if(MOUSEDOWN){
    if(selected_slot = max_inventory-1){
        selected_slot = 0;
    }else{
        selected_slot++
    }
}
if(MOUSEUP){
    if(selected_slot = 0){
        selected_slot = max_inventory-1;
    }else{
        selected_slot--
    }
}
//^^^^^^inventory m e c h a n i c s

//gamer time, code below makes the items actually do something
if(RPMOUSE){
    switch(inventory_array[selected_slot,0]){
        case 0:
        current_hydration += 20;
        inventory_array[selected_slot,1]--;
        if(inventory_array[selected_slot,1] == 0)inventory_array[selected_slot,0] = -1;
        break;
        case 1:
        current_saturation += 20;
        inventory_array[selected_slot,1]--;
        if(inventory_array[selected_slot,1] == 0)inventory_array[selected_slot,0] = -1;
        break;
        default:
        break;
    }
}

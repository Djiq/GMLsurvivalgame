        if(secondary_inventory_array[i,0] != -1){
            draw_set_color(c_white);
            var xscaling = 32/16;
            var yscaling = 32/16;
            draw_sprite_ext(sItems,secondary_inventory_array[i,0],64+(k*32),160+((i - (8*k))*32),xscaling,yscaling,0,c_white,1);
            draw_text(64+(k*32),176+((i - (8*k))*32),secondary_inventory_array[i,1]);
          
        }
    }
}
draw_set_color(c_black);

if(has_in_hand[0] != -1){
    var xscaling = 1;
    var yscaling = 1;
    draw_sprite_ext(sItems,has_in_hand[0], (mouse_x - view_xview)*2,(mouse_y - view_yview)*2 ,xscaling,yscaling,0,c_white,1);


}







//DEBUG CODE
//
if(DEBUG){
draw_text(view_wport/2,32,"DEBUG MODE ON");
draw_text(view_wport/2,64,current_health);
draw_text(view_wport/2,96,current_stamina);
draw_text(view_wport/2,128,current_saturation);
draw_text(view_wport/2,160,current_hydration);
draw_text(view_wport/2,192,mouse_x);
draw_text(view_wport/2,224,mouse_y);
}
